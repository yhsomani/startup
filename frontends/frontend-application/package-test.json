{
  "name": "talentsphere-frontend-tests",
  "version": "1.0.0",
  "description": "React Testing Suite for TalentSphere Frontend",
  "main": "index.js",
  "scripts": {
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll=false --watchPath='src'",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:unit": "react-scripts test --testPathPattern='src/**/__tests__/**/*.test.js'",
    "test:integration": "react-scripts test --testPathPattern='src/**/__integration__/**/*.test.js'",
    "test:e2e": "react-scripts test --testPathPattern='src/**/__e2e__/**/*.test.js'",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci --coverage",
    "test:verbose": "react-scripts test --verbose",
    "test:debug": "react-scripts test --runInBand",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:quiet": "eslint src --ext .js,.jsx,.ts,.tsx --quiet",
    "test:dev": "npm run test:watch",
    "test:build": "npm run build && npm run test:ci",
    "clean": "rimraf build/ coverage/ test-results",
    "validate": "npm install && npm test:unit",
    "validate:lint": "npm run lint && npm run test:unit"
    "pretest": "npm run clean",
    "posttest": "npm run test:coverage:report"
    "test:coverage:report": "codecov --dry-run",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "serve -s build"
  },
  "dependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.0.2",
    "@testing-library/dom-testing-library": "^5.16.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-matchers": "^5.0.1",
    "@testing-library/react-renderer": "^13.0.0",
    "msw": "^2.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.20",
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0",
    "@mui/x-date-pickers": "^6.18.2",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.2",
    "recharts": "^2.8.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.1",
    "react-intersection-observer": "^9.5.3",
    "react-virtualized": "^9.22.5",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/node": "^20.10.4",
    "@types/lodash": "^4.14.202",
    "@testing-library/jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.0.2",
    "@testing-library/dom-testing-library": "^5.16.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-matchers": "^5.0.1",
    "@testing-library/react-renderer": "^13.0.0",
    "@testing-library/dom-testing-library": "^5.16.5",
    "@testing-library/user-event": "^14.0.2",
    "@types/jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.32.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jest": "^27.6.3",
    "prettier": "^3.1.0",
    "react-scripts": "5.0.1",
    "jest": "^29.7.0",
    "cypress": "^13.8.2",
    "playwright": "^1.40.0",
    "jest-environment-jsdom": "^29.7.0",
    "@storybook/testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.2"
    "husky": "^8.0.3",
    "lint-staged": "*.{js,jsx,ts,tsx}",
    "@percyte/storybook": "^7.6.17"
    "start-server": "node mock-server.js",
    "test:mock-server": "node mock-server.js",
    "test:e2e: "npx cypress run",
    "test:e2e:headed": "npx cypress run --headed",
    "test:e2e:ci": "npx cypress run --record --spec --reporter mochawesome/cypress-mochawesome-json"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
  }
}