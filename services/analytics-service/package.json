{
    "name": "talentsphere-analytics-service",
    "version": "1.0.0",
    "description": "Analytics and reporting service for TalentSphere platform",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "test:unit": "jest --testPathPattern=unit",
        "test:integration": "jest --testPathPattern=integration",
        "test:coverage": "jest --coverage",
        "test:report": "npm run test:coverage && node scripts/generate-coverage-report.js",
        "coverage:visualize": "node scripts/generate-coverage-report.js",
        "lint": "eslint ."
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "moment": "^2.29.4",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "@babel/preset-env": "^7.28.6",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "keywords": [
        "talentsphere",
        "analytics",
        "reporting",
        "dashboard",
        "metrics",
        "user-engagement",
        "business-intelligence"
    ],
    "author": "TalentSphere Team",
    "license": "MIT"
}
